{
  "name": "docker-client/client",
  "license": "MIT",
  "type": "library",
  "description": "A Docker PHP client",
  "autoload": {
    "psr-4": {
      "Docker\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Docker\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": "^7.4",
    "php-http/socket-client": "^2.0",
    "php-http/client-common": "^2.3",
    "guzzlehttp/psr7": "^1.7",
    "docker-client/open-api": "6.1.40.*",
    "splitbrain/php-archive": "^1.2.0",
    "psr/log": "^1.1.3",
    "react/socket": "^1.6",
    "react/http": "^1.1",
    "psr/http-client": "^1.0.1",
    "clue/block-react": "^1.4.0",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0"
  },
  "prefer-stable": true,
  "minimum-stability": "dev"
}
